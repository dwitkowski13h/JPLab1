#include <stdlib.h>
#include <stdio.h>
#include <math.h>
#include <complex.h>
int main(void) {
  double a,b; 

  scanf("%lf %lf", &a, &b);
  if(a<b)
  {
      printf("Liczba %.2lf jest mniejsza od %.2lf", a,b);
  }
      else
    {
        printf("Liczba %.2lf jest wieksza od %.2lf", a,b);
    }

return 0;
}








